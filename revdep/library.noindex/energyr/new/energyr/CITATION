citHeader("To cite energyr in publications use:")

bibentry(
  bibtype    = "Manual",
  title    = "energyr: Data Published by the United States Federal Energy Regulatory
Commission",
  author   = "Paul Govan",
  year     = "2018",
  note     = "R package version 0.1.2",
  url      = "https://CRAN.R-project.org/package=energyr",
  doi      = "10.32614/CRAN.package.energyr"
)
